<%if(success && success.length){%>
<div class="alert alert-success offset-3 col-6 alert-dismissible fade show" role="alert" id="msg"  >
    
    <%=success%>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>

    
</div>
<%}%>

<%if(failure && failure.length){%>
    <div class="alert alert-danger offset-3 col-6 alert-dismissible fade show" role="alert" id="msg" >
        
        <%=failure%>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    
        
    </div>
    <%}%>

    <%if(error && error.length){%>
        <div class="alert alert-danger offset-3 col-6 alert-dismissible fade show" role="alert" id="msg" >
            
            <%=error%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        
            
        </div>
        <%}%>

 
    
   